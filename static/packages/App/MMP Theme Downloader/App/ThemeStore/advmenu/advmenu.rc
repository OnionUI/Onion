event_assign difficulty none
misc_quiet yes
mode tile_tiny
mode_skip full_mixed text list_mixed tile_icon tile_marquee
#list_mixed title_enormous title_giant title_icon title_marquee
sound_background_loop none
sound_background_loop_dir ./
