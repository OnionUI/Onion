# Special commands
ui_command "Force Thumbnails refresh" cd ..; ./thumbnails_generator.sh 1; ./bin/say "Please restart Easy LogoTweak"; sleep 3; pkill -9 advmenu; 

emulator "EasyLogoTweak" generic "../themeadd.sh" "%p"
emulator_roms "EasyLogoTweak" "./thumbnails/"
emulator_roms_filter "EasyLogoTweak" "*.png:*.pcx"
emulator_altss "EasyLogoTweak"  "./thumbnails/"
emulator_flyers "EasyLogoTweak"  "./thumbnails/"
easylogotweak/mode tile_small
easylogotweak/sort name
emulator_include "EasyLogoTweak"
group "<undefined>"
type "<undefined>"
emulator_attrib "EasyLogoTweak" missing exclude
emulator_attrib "EasyLogoTweak" duplicate include
game "EasyLogoTweak/151" "" "" 0 1 ""
